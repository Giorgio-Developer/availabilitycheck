    "test:integration": "jest --testPathPattern=integration",
    "test:unit": "jest --testPathPattern=unit",
    "test:admin": "jest tests/integration/admin.test.js",
    "test:routes": "jest tests/integration/calendar.route.test.js tests/integration/freebusy.route.test.js",
    "test:utils": "jest tests/unit/dateUtils.test.js tests/unit/csvUtils.test.js tests/unit/translate.test.js",
    "test:booking": "jest tests/unit/BookingHelper.test.js",
    "test:google": "jest tests/unit/GoogleCalendar.test.js",
    "test:app": "jest tests/integration/app.test.js",
    "test:install": "node test-runner.js install",
    "test:verbose": "jest --verbose",
    "test:debug": "jest --detectOpenHandles --verbose"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.12.2",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "express": "^4.19.2",
    "express-session": "^1.18.0",
    "googleapis": "^140.0.0",
    "json2csv": "^6.0.0-alpha.2",
    "moment": "^2.30.1",
    "multer": "^2.0.2",
    "openai": "^5.20.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "supertest": "^6.3.4"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "**/*.js",
      "!node_modules/**",
      "!coverage/**",
      "!tests/**",
      "!jest.config.js"
    ],
    "testMatch": [
      "**/tests/**/*.test.js",
      "**/tests/**/*.spec.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setup.js"
    ]
  }
